    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  },
  "keywords": [
    "data-masking",
    "privacy",
    "security",
    "typescript",
    "pii",
    "gdpr",
    "card-number",
    "email-masking",
    "phone-masking",
    "jwt",
    "uuid",
    "json-masking"
  ],
